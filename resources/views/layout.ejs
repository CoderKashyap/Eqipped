    <script  >


function india(aww) {
          $('#searchProduct').val(aww);
          document.getElementById("submitSearch").click()
      }

function sendData(e) {
        const searchResults = document.getElementById('searchResults')
        let match = e.value.match(/^[a-zA-Z ]*/);
        let match2 = e.value.match(/\s*/);
        if (match2[0] === e.value) {
          searchResults.innerHTML = '';
          return;
        }
        if (match[0] === e.value) {
          fetch('getProductNamesForSearch', {
            method: "POST",
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify({ payload: e.value })
          }).then(res => res.json()).then(data => {
            let payload = data.payload;
            searchResults.innerHTML = '';
            if (payload.length < 1) {
              searchResults.innerHTML = '<p>Sorry, Nothing Found.</p>';
              return;
            }
            payload.forEach((item, index) => {
              if (index > 0) searchResults.innerHTML += '<hr>';
              searchResults.innerHTML += `<div onclick="india('${item.pname}')" class="flex px-2 cursor-pointer items-center w-28"> <img class="h-10 py-1 object-contain"
                            src="/img/${ item.image[0].imgName}" alt="${item.pname}" />
                             <span class="p-2 px-4 text-xs">${item.pname}</span></div>`
            });
          });
          return;
        }
        searchResults.innerHTML = '';
      }

      

      $(document).ready(function () {
        if (screen.width > 500) {
          $('#screenSidebarIcon').append("  <i class='bx bx-chevron-right '>");
        } else {
          $('#screenSidebarIcon').append('<svg class=" w-6 h-6" fill="currentColor" viewBox="-2.5 -2 25 25" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M3 5a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 10a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 15a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z" clip-rule="evenodd"></path></svg>');
        }
      });
      function preloadFunc() {
        const url_split = ('' + window.location).split('/');
        const ActivePage = url_split[url_split.length - 1];
        switch (ActivePage) {
          case 'home':
            $("#navhome").addClass("nav-active");
            break;
          case 'orders':
            $("#navorder").addClass("nav-active");
            break;
          case 'cart':
            $("#navcart").addClass("nav-active");
            break;
          case "favourite":
            $("#navfav").addClass("nav-active");
            break;
          case 'verification':
            $("#navver").addClass("nav-active");
            break;
          case 'notify':
            $("#nav-popbutton").addClass("nav-active");
            break;
          case 'vendordashboard':
            $("#navvendordash").addClass("nav-active");
            break;
          case 'addproduct':
            $("#navaddprod").addClass("nav-active");
            break;
          case 'listedProduct':
            $("#naveditprod").addClass("nav-active");
            break;
          default:
            break;
        }
      }
      window.onpaint = preloadFunc();
      // autocomplete script for search-bar 
      $("#nav-popbutton").hover(function () {
        // $("#nav-popover").css("display", "flex");
      }, function () {
        $("#nav-popover").css("display", "none");
      });
      $(function () {
        $('#searchProduct').autocomplete({
          source: function (req, res) {
            $.ajax({
              url: "autocomplete/",
              dataType: "jsonp",
              type: "GET",
              data: req,
              success: function (data) {
                res(data)
              },
              error: function (err) {
                console.log(err.status)
              }
            });
          },
          minLength: 1,
          select: function (event, ui) {
            if (ui.items) {
              $('#searchProduct').text(ui.item.label);
            }
          }
        });
      });
      $(document).ready(function () {
        $(".owl-carousel-big").owlCarousel({
          loop: true,
          responsiveClass: true,
          responsive: {
            0: {
              items: 2.3,
              nav: false
            },
            600: {
              items: 3.1,
              nav: false
            },
            1000: {
              items: 4.8,
              nav: false,
              loop: true
            }
          }
        });
      });
